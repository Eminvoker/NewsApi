<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CardView">
        <!-- 在v20和之前的版本中添加内边距，这个属性是为了防止卡片内容和边角的重叠 -->
        <attr name="cardPreventCornerOver" format="boolean"></attr>
        <!-- 设置内边距，v21+的版本和之前的版本仍旧具有一样的计算方式 -->
        <attr name="cardUserCompatPadding" format="boolean"></attr>
        <attr name="cardCornerRadius" format="dimension"></attr>
        <attr name="cardElevation" format="dimension"></attr>
        <attr name="cardBackgroundColor" format="color"></attr>
    </declare-styleable>
</resources>